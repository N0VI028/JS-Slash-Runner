{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "调试酒馆网页-chrome",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "开始监听源代码并编译",
      "postDebugTask": "结束监听源代码并编译",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}/../../../../",
      "sourceMapPathOverrides": {
        "src://tavern_helper/*": "${workspaceFolder}/*",
        "webpack://tavern_helper/*": "${workspaceFolder}/*",
      },
      "disableNetworkCache": true,
      "internalConsoleOptions": "openOnSessionStart",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "调试酒馆网页-edge",
      "type": "msedge",
      "request": "launch",
      "preLaunchTask": "开始监听源代码并编译",
      "postDebugTask": "结束监听源代码并编译",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}/../../../../",
      "sourceMapPathOverrides": {
        "src://tavern_helper/*": "${workspaceFolder}/*",
        "webpack://tavern_helper/*": "${workspaceFolder}/*",
      },
      "disableNetworkCache": true,
      "internalConsoleOptions": "openOnSessionStart",
      "cwd": "${workspaceFolder}",
    }
  ]
}
